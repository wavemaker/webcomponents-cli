{
  "name": "@wavemaker/webcomponents-cli",
  "version": "0.0.29",
  "description": "A CLI to convert WaveMaker Prefabs into WebComponents",
  "main": "index.js",
  "files": [
    "/dist",
    "./index.js"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:prod": "webpack --config webpack.config.prod.js",
    "build:dev": "webpack --config webpack.config.dev.js",
    "pack:prod": "npm run build:prod && npm pack",
    "pack:dev": "npm run build:dev && npm pack",
    "start": "npm run build:dev && node ."
  },
  "keywords": [
    "WaveMaker",
    "CLI",
    "Prefabs",
    "WebComponents"
  ],
  "bin": {
    "webcomponents-cli": "./index.js"
  },
  "author": "naresh.ravulapalli@gmail.com",
  "license": "ISC",
  "dependencies": {
    "acorn": "^8.12.0",
    "archiver": "^7.0.1",
    "chalk": "^3.0.0",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "estraverse": "^5.3.0",
    "figlet": "^1.7.0",
    "handlebars": "^4.7.8",
    "inquirer": "^7.0.4",
    "minimist": "^1.2.0",
    "ncp": "^2.0.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "rimraf": "^3.0.2",
    "xml2js": "^0.6.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^12.0.2",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "5.76.1",
    "webpack-cli": "5.1.4",
    "webpack-merge": "^6.0.1",
    "webpack-node-externals": "3.0.0"
  },
  "engines": {
    "node": ">=18.16.1",
    "npm": ">=9.5.1"
  },
  "engineStrict": true
}
